{
  "entryPoints": [
    "src/app/**/*.ts"
  ],
  "exclude": [
    "**/*.spec.ts",
    "**/node_modules/**",
    "**/dist/**"
  ],
  "out": "docs/frontend",
  "name": "Sistema de Gesti√≥n de Activos - Frontend",
  "readme": "README.md",
  "theme": "default",
  "includeVersion": true,
  "excludePrivate": true,
  "excludeProtected": false,
  "excludeInternal": false,
  "categorizeByGroup": true,
  "categoryOrder": [
    "Core",
    "Features",
    "Shared",
    "Services",
    "Models",
    "Guards",
    "Interceptors"
  ],
  "sort": [
    "source-order"
  ],
  "kindSortOrder": [
    "Project",
    "Module",
    "Namespace",
    "Enum",
    "Class",
    "Interface",
    "TypeAlias",
    "Constructor",
    "Property",
    "Method",
    "Accessor",
    "Function",
    "Variable"
  ]
}

